{
  "name": "@ridi/expo-app",
  "main": "index.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start-clear": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "version-update": "node ./scripts/version-create.mjs",
    "build:android": "eas build --profile development --platform android",
    "build:web": "expo export -p web --dump-sourcemap && pnpm run version-update && sentry-expo-upload-sourcemaps dist",
    "release:web:new": "sentry-cli releases new \"ridi-apps-expo@$(sentry-cli releases propose-version)\" && sentry-cli releases set-commits \"ridi-apps-expo@$(sentry-cli releases propose-version)\" --auto --ignore-missing",
    "release:web:fin": "sentry-cli releases finalize \"ridi-apps-expo@$(sentry-cli releases propose-version)\"",
    "tsc": "tsc --watch",
    "type-check": "node ./scripts/type-check.mjs"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
